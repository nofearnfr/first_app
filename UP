import xlrd
from mainproduct.models import *
bulunan_bu_urun = []
gitmeyen = []
wb = xlrd.open_workbook("/home/enver.turkan/mudo_barcode.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%row[0]
    pms = ProductMeta.objects.filter(productmetabarcode__barcode=barkod).distinct()
    if pms:
        pms = pms[0]
        if pms.check_vertical():
            if pms.product.id not in bulunan_bu_urun:
                bulunan_bu_urun.append(pms.product.id)
for x in bulunan_bu_urun:
    p = Product.objects.get(id=x)
    p.is_active = False
    p.save()
    try:
        p.sent_product_info()
    except:
        gitmeyen.append(p.id)
print gitmeyen
--
