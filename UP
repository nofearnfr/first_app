products = Product.objects.filter(urlkey__url__in=a)
for product in products:
    for pm in product.productmeta_set.all():
        sample_items = []
        ayakkabi_erkek = [42, 43, 41, 44, 45]
        ayakkabi_kadin = [37, 38, 36, 39, 40]
        ayakkabi_cocuk = [32, 31, 24, 23, 25, 28, 27, 18, 17, 19, 20]
        ayakkabi_genel = [34, 35, 46]
        aksesuar_erkek = ['M','L','S']
        aksesuar_kadin = ['S','M','L']
        aksesuar_genel = ['Standart']
        tekstil_erkek = aksesuar_erkek + ["32", "32-32", "30-32", "31-32", "32-31", "40", "40-42", "44", "42", "46", "48", "50", "52", ]
        tekstil_kadin = ['S', 'M', 'L', '26','27','28','29','30','36', '34', '38', '40', '42', '44', '46', '1', '2', '3', "26-28", "26-30","26-32","26-34" "27-30","28-30","29-30", "30-30", "26-32", "27-32","28-32","29-32","30-32"]
        tekstil_genel = aksesuar_genel
        use_list = []
        break_list = False
        pms = product.productmeta_set.all()
        po = product.productoptions_set.filter(name__name__in = [u'Cinsiyet', u'CİNSİYET'])
        if pms.count() > 1:
            if product.mainproduct.product_type.name == u"Ayakkabı":
                use_list = ayakkabi_erkek + ayakkabi_kadin + ayakkabi_cocuk + ayakkabi_genel
                if po:
                    po = po[0]
                    if po.value.value == 'Erkek':
                        use_list = ayakkabi_erkek
                    elif po.value.value == u'Kadın':
                        use_list = ayakkabi_kadin
                    elif po.value.value == u'Çocuk':
                        use_list = ayakkabi_cocuk
            elif product.mainproduct.product_type.name == u"Aksesuar":
                use_list = aksesuar_genel + aksesuar_erkek + aksesuar_kadin
                if po:
                    po = po[0]
                    if po.value.value == 'Erkek':
                        use_list = aksesuar_erkek
                    elif po.value.value == u'Kadın':
                        use_list = aksesuar_kadin
            elif product.mainproduct.product_type.name == u"Tekstil":
                use_list = tekstil_genel + tekstil_erkek + tekstil_kadin
                if po:
                    po = po[0]
                    if po.value.value == 'Erkek':
                        use_list = tekstil_erkek
                    elif po.value.value == u'Kadın':
                        use_list = tekstil_kadin
            for no in use_list:
                pmeta = product.productmeta_set.filter(option_value__value=no) if product.productmeta_set.filter(option_value__value=no) else product.productmeta_set.filter(option_value__value__startswith=no)
                if pmeta:
                    pmeta = pmeta[0]
                    sample_items.append(pmeta)
                    break_list = True
                    break
            if not break_list:
                print "yok",product.id
                #sample_items.append(pm)
        elif pms.count() == 1:
            sample_items.append(pm)
        if pm in sample_items:
            print "True", pm.barcode, product.id


