hatali = []   
for x in a:
    p = Product.objects.get(productmeta__productmetabarcode__barcode=x[0])
    po = p.productoptions_set.filter(name=pon)
    if po:
        try:
            po = po[0]
            if x[1] == u"KADIN" and po.value == erkek:
                po.value = kiz
                po.save()
                for pm in p.productmeta_set.all():
                    for bi in pm.basketitem_set.all():
                        bo = bi.basketitemoptions_set.filter(name=u"Cinsiyet")
                        if bo:
                            bo = bo[0]
                            bo.value = kiz.value
                            bo.save()
                p.update_summary()
                if p.is_sent and p.is_published:
                    p.sent_product_info()
            elif x[1] == u"ERKEK" and po.value == kiz:    
                po.value = erkek
                po.save()
                for pm in p.productmeta_set.all():
                    for bi in pm.basketitem_set.all():
                        bo = bi.basketitemoptions_set.filter(name=u"Cinsiyet")
                        if bo:
                            bo = bo[0]
                            bo.value = erkek.value
                            bo.save()
                p.update_summary()
                if p.is_sent and p.is_published:
                    p.sent_product_info()
            else:
                pass
        except:
            hatali.append(p.id)        
    else:                    
        if x[1] == u"KADIN":
            p.productoptions_set.create(name=pon,value=kiz)
        elif x[1] == u"ERKEK":    
            p.productoptions_set.create(name=pon,value=erkek)
        else:                    
            pass


