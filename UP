import xlrd
from mainproduct.models import *
pn = ProductOptionName.objects.get(name="SEZON")

f = open("/tmp/sezon-60-114ek.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/sezon2.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%row[0]
    pmb = ProductMetaBarcode.objects.filter(barcode="%s"%barkod)
    if pmb:
        pmb = pmb[0]
        p = pmb.product_meta.product
        po = p.productoptions_set.filter(name=pn)
        if po:
            po = po[0]
            sezon = po.value.value
        else:
            sezon = u"sezon yok"
    else:
        sezon = u"barkod bulunamadi"
    k = "%s=%s\n" %(barkod, sezon)
    f.write(k.encode("utf-8","replace"))

f.close()

