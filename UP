####################################################################################################################
#############################                                        ###############################################
#############################  Fraud Müşteriler ve Siparişleri  SQL  ###############################################
#############################                                        ###############################################
####################################################################################################################
####################################################################################################################
####################################################################################################################
####################################################################################################################

select c.name, c.email, o.tracker_code, bi.barcode, o.is_cargo_sent, o.is_returned, o.is_cancelled
from customer_customer c, order_order o, order_basketitem bi
where c.options ? 'is_fraud'
and c.options @> '"is_fraud"=>"True"'::hstore
and o.customer_id = c.id
and bi.order_id = o.id
group by 1,2,3,4,5,6,7
order by 1

