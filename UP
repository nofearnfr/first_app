for p in Product.objects.all():
   ....:     l = Log.objects.filter(model_value=p.id, model_name="Product", action_name=u"Ürün Siteye Gönderildi").order_by("-id")
   ....:     if l:
   ....:         l = l[0]
   ....:         p.sent_at = datetime.datetime(l.action_date.year,l.action_date.month,l.action_date.day, l.action_time.hour,l.action_time.minute,l.action_time.second)
   ....:         p.save()




for p in Product.objects.filter(is_sent=True):
   ...:     l = Log.objects.filter(model_value=p.id, model_name="Product", action_name=u"Ürün Siteye Gönderildi").order_by("-id")
   ...:     if l:
   ...:         p.sent_at = datetime.datetime(l.action_date.year,l.action_date.month,l.action_date.day, l.action_time.hour,l.action_time.minute,l.action_time.second)
   ...:         p.save()
   ...:     if not l and p.published_at:    
   ...:         p.sent_at = p.published_at
   ...:         p.save()
   ...:     if not l and not p.published_at:    
   ...:         sql = """ select created_at from product_product where erp_id=%s""" % p.id
   ...:         cursor.execute(sql)
   ...:         z = cursor.fetchone()
   ...:         if z:
   ...:             p.published_at = z[0]
   ...:             p.sent_at = z[0]
   ...:             p.save()

