from mainproduct.models import *
from mainproduct.views import cell_type_control
pn = ProductOptionName.objects.get(name="RENK")
import xlrd
f = open("/tmp/consignment_renk.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/consignment.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%cell_type_control(row[3])
    p = Product.objects.filter(productmeta__productmetabarcode__barcode=barkod)
    if p:
        p = p[0]
        po = p.productoptions_set.filter(name=pn)
        if po:
            renk = po[0].value.value
            k = "%s=%s\n" %(barkod, renk)
        else:
            k = "%s=yok\n" %(barkod)
    else:
        k = "%s=bulunamadi\n" %(barkod)
    f.write(k.encode("utf-8","replace"))



from mainproduct.models import *
from mainproduct.views import cell_type_control
import xlrd
desc = ""
f = open("/tmp/miss_description.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/miss_aciklama.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%cell_type_control(row[0])
    p = Product.objects.filter(productmeta__productmetabarcode__barcode=barkod)
    if p:
        p = p[0]
        desc = p.description         
        k = "%s=%s\n" %(barkod, desc)
    else:
        k = "%s=urun bulunamadi\n" %(barkod)
    f.write(k.encode("utf-8","replace"))


