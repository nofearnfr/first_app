################################################################################################################
################################################################################################################
######################################                                      ####################################
######################################   RAPOR tedarikçilerin markaları     ####################################
######################################                                      ####################################
################################################################################################################
################################################################################################################


select sp.name, br.name

from supplier_supplier sp,
     brand_brand br,
     order_order o,
     order_basketitem bi
where bi.order_id = o.id
and bi.brand = br.name
and o.supplier_id = sp.id
group by sp.name, br.name

order by sp.name


