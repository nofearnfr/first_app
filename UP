#####################################                         ##################################################
#####################################    KEYWORD TANIMLAMA    ##################################################
#####################################                         ##################################################
################################################################################################################
################################################################################################################


p = Product.objects.filter(mainproduct__brand__name)

for i in p:
    if i.keywords:
        i.keywords = i.keywords + ",dexta"
        i.save()
    else:
        i.keywords = "dexta"
        i.save()

