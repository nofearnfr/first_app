import xlrd
from mainproduct.models import *
pn = ProductOptionName.objects.get(name="TEDARİKÇİ CİNSİYET")

f = open("/tmp/cinsiyet_kdv.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/cins_kdv.xls")
sh1 = wb.sheet_by_index(0)
vat = ""
cins = ""
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%row[0]
    pmb = ProductMetaBarcode.objects.filter(barcode="%s"%barkod)
    if pmb:
        pm = pmb[0].product_meta
        p = pm.product
        vat = p.price.vat
        cins = p.productoptions_set.filter(name=pn)
        if cins:
            cins = cins[0].value
        else:
            cins = "yok"
    else:
        vat = "bakod bulunamadi"
        cins = "barkod bulunamadi"
    k = "%s=%s=%s\n" %(barkod, cins, vat)
    f.write(k.encode("utf-8","replace"))
f.close()


import xlrd
from mainproduct.models import *
from mainproduct.views import cell_type_control
f = open("/tmp/konsinyeler2.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/konsinyemi.xls")
sh1 = wb.sheet_by_index(0)
k = ""
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%cell_type_control(row[0])
    pm = ProductMeta.objects.filter(productmetabarcode__barcode=barkod)
    if pm:
        pm = pm[0]
        pms = pm.productmetasummary_set.all()
        if pms:
            pms = pms[0]
            if pms.consignment_buying_waybill_count > 0:
                k = "%s=Konsinye\n" %(barkod)
            else:
                k = "%s=Kesin\n" %(barkod)
        else:
            k = "%s=pms yok\n" %(barkod)
    else:
        barkod = "0"+str(barkod)
        pmm = ProductMeta.objects.filter(productmetabarcode__barcode=barkod)
        if pmm:
            pmm = pmm[0]
            pmss = pmm.productmetasummary_set.all()
            if pmss:
                pmss = pmss[0]
                if pmss.consignment_buying_waybill_count > 0:
                    k = "%s=Konsinye\n" %(barkod)
                else:
                    k = "%s=Kesin\n" %(barkod)
            else:
                k = "%s=pmss yok\n" %(barkod)
        else:
            k = "%s=pmm yok\n" %(barkod)

    f.write(k.encode("utf-8","replace"))    
f.close()

