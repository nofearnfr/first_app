for x in a:
    p = Product.objects.get(productmeta__productmetabarcode__barcode=x[0])
    if not p.productoptions_set.filter(name=cinsiyet):
        if x[1] == u"KADIN":
            p.productoptions_set.create(name=cinsiyet,value=kadin)
            for pm in p.productmeta_set.all():
                for bi in pm.basketitem_set.all():
                    if bi.options:
                        bi.options["cinsiyet"] = u"Kadın"
                    else:    
                        bi.options = {"cinsiyet": u"Kadın"}
                    bi.save(update_fields=["options"])    
        elif x[1] == u"ERKEK":             
            p.productoptions_set.create(name=cinsiyet,value=erkek)
            for pm in p.productmeta_set.all():
                for bi in pm.basketitem_set.all():
                    if bi.options:
                        bi.options["cinsiyet"] = u"Erkek"
                    else:    
                        bi.options = {"cinsiyet": u"Erkek"}
                    bi.save(update_fields=["options"])    
        elif x[1] == u"ÇOCUK":            
            p.productoptions_set.create(name=cinsiyet,value=cocuk)
            for pm in p.productmeta_set.all():
                for bi in pm.basketitem_set.all():
                    if bi.options:
                        bi.options["cinsiyet"] = u"Çocuk"
                    else:    
                        bi.options = {"cinsiyet": u"Çocuk"}
                    bi.save(update_fields=["options"])    
        p.update_summary()
        if p.is_sent and p.productimage_set.all() and p.has_stock():
            p.send()


