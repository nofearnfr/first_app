for x in a:
    pmb = ProductMetaBarcode.objects.get(barcode=x[1])
    mp = pmb.product_meta.product.MainProduct
    if mp.code != x[0]:
        mp.code = x[0]
        mp.special_code = x[0]
        mp.save(update_fields=["code","special_code"])
        for yy in mp.product_set.all():
            for zz in yy.productmeta_set.all():
                for kk in zz.basketitem_set.all():
                    kk.code = x[0]
                    kk.special_code = x[0]
                    kk.save(update_fields=["code","special_code"])
                zz.update_summary_table(force_update=True, use_celery=False)


