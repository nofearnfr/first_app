####################################################################################################################
########################																	 #######################
########################     tedarikçi bilgileri konsinye mi kesin mi tedarikçi markaları    #######################
########################																	 #######################
####################################################################################################################
####################################################################################################################
####################################################################################################################
####################################################################################################################



select s.id, s.name, bi.product_type, i.is_consignment, array_agg(distinct bi.brand)
from order_order o, order_basketitem bi, invoice_invoice i, invoice_invoiceproducts ip, supplier_supplier s
where o.type = 2
       and o.id = bi.order_id
       and (bi.options->'shipment')::numeric = ip.id
       and ip.invoice_id = i.id
       and i.supplier_id = s.id
group by 1,2,3,4


