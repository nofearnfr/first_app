f = open("/tmp/urunler.csv","wb")

for x in Product.objects.filter(productcategory__category=c):
        for i in x.productmeta_set.all():
        k = "%s=%s=%s\n" %(i.product.mainproduct.code, i.product.mainproduct.brand, i.barcode)
        f.write(k.encode("utf-8","replace"))

f.close()

