####################################################################################################################
####################################################################################################################
####################################################################################################################
####################################################################################################################
#####################################                                   ############################################
#####################################      müşteri listesi verme  SQL   ############################################
#####################################                                   ############################################
####################################################################################################################
####################################################################################################################
####################################################################################################################
####################################################################################################################

ürünün kategorilerine göre

select cc.name, cc.email, cc.gender
from    
    order_order o
    , order_basketitem bi
    ,customer_customer cc
    ,mainproduct_productcategory pc
    ,mainproduct_product pp
    ,mainproduct_productmeta pm
    
where
    bi.order_id = o.id
    bi.product_meta_id = pm.id
    pm.product_id = pp.id
    and bi.brand = 'DC Shoes' --('Etnies','Emerica','Vans','DC Shoes')
    and o.customer_id = cc.id
    and o.type in  (2,4,5,6)
    and pc.id in (132,181,176,140,147,189)




markalarına göre

select cc.name, cc.email, cc.gender
from    
    order_order o
    , order_basketitem bi
    ,customer_customer cc
where
    bi.order_id = o.id
    and bi.brand = 'Etnies' --('Etnies','Emerica','Vans','DC Shoes')
    and o.customer_id = cc.id
    and o.type in  (2,4,5,6)
group by 1,2,3
order by 3

