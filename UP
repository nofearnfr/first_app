################################################################################################################

o = Order.objects.filter(tracker_code__in=("S514","S513", "S507", "S506", "S483", "S482", "S481", "S477", "S474", "S473", "S472", "S454", "S444", "S443", "S431", "S420", "S419", "S418", "S415"))

pon = ProductOptionName.objects.filter(name="SEZON")[0]
pov = ProductOptionValue.objects.filter(value="İ/Y 2013")[0]

for i in Product.objects.filter(productmeta__basketitem__order__in=o):
    ps = i.productoptions_set.filter(name=pon)
    if ps:
        ps=ps[0]
        ps.value = pov
        ps.save()
    else:
        i.productoptions_set.create(name=pon, value=pov)
    for x in i.productmeta_set.all():
        for y in x.basketitem_set.filter(order__in=o):
            bi = y.basketitemoptions_set.filter(name=pon.name)
            if bi:
                bi = bi[0]
                bi.value = pov.value
                bi.save()
            else:
                y.basketitemoptions_set.create(name=pon.name, value=pov.value)

