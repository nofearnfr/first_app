##################################                                                 #############################
##################################    barkodların tedarikçi bilgileri excelden     #############################
##################################                                                 #############################
################################################################################################################
################################################################################################################


import xlrd
from order.models import *
from mainproduct.views import cell_type_control

f = open("/tmp/barkod_tedarikci.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/bar_ted.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    barkod = u"%s"%cell_type_control(row[0])
    bi = BasketItem.objects.filter(order__type=1, barcode=barkod).order_by("-id")
    if bi:
        bi = bi[0]
        if bi.order.supplier:
            k = "%s=%s\n" %(barkod, bi.order.supplier)
        else:
            k = "%s=tedarikcibulunamadi\n" %(barkod)
    else:
        k = "%s=barkodbulunamadi\n" %(barkod)
    f.write(k.encode("utf-8","replace"))
f.close()





for x in a:
   ....:     bi = BasketItem.objects.filter(order__type=1, barcode=x).order_by("-id")
   ....:     if bi:
   ....:         bi = bi[0]
   ....:         print bi.barcode,"=",bi.order.supplier
   ....:     else:
   ....:         print x,"yok"

