import xlrd
from mainproduct.models import *
from order.models import *
f = open("/tmp/logo-id-barcode-order-cons.csv","wb")
wb = xlrd.open_workbook("/home/enver.turkan/logoidzzg.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(0,sh1.nrows):
    row = sh1.row_values(rownum)
    ids = int(row[0])
    pm = ProductMeta.objects.filter(id=ids)
    if pm:
        pm = pm[0]
        barkod = pm.barcode
        bi = BasketItem.objects.filter(barcode = barkod, order__type = 1)
        ord = u""
        kons = u"hayir"
        for x in bi:
            if ord:
                ord = ord + "," + x.order.tracker_code
            else:
                ord = x.order.tracker_code
            if x.order.po.is_consignment:
                kons = u"evet"
    else:
        barkod = u"barkodu bulunamadi"
    k = "%s=%s=%s=%s\n" %(ids, barkod, kons, ord)
    f.write(k.encode("utf-8","replace"))
f.close()



