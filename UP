i = o.invoice_set.get()

for x in a:
	bi = o.basketitem_set.filter(barcode=x[0])
   	if bi:
   		bi = bi[0]
   		ip = bi.invoiceproducts_set.get()
   		bi.selling_price = x[1]
   		ip.selling_price = x[1]
   		bi.save(update_fields=["selling_price"])
   		ip.save(update_fields=["selling_price"])
   		bi.calculate_necessary_fields()
   		ip.calculate_necessary_fields()         

o.calculate_necessary_fields()
i.calculate_necessary_fields()


   

