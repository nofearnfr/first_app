import xlrd
from mainproduct.models import *
from customer.models import *
from mainproduct.views import cell_type_control
from order.models import *
from syncer.data_importer import *
tracker_code = ""
wb = xlrd.open_workbook("/home/enver.turkan/enmodasatiss.xls")
sh1 = wb.sheet_by_index(0)
aa = 1
for rownum in range(0,sh1.nrows):

    row = sh1.row_values(rownum)
    email = u"%s"%row[0]
    barkod = u"%s"%cell_type_control(row[1])
    price = float(u"%s"%row[2])
    pmb = ProductMetaBarcode.objects.get(barcode=barkod)
    pm = pmb.product_meta
    product = pm.product
    pr = product.price
    c = Customer.objects.filter(email=email)

    if c:
        customer = c[0]
    else:
        customer = Customer.objects.create(email=email)
        customer.name = email.split("@")[0]
        customer.save()
    
    tracker_code = "ENM1010" + str(customer.id) + str(aa)
    aa += 1

    data = {"data": {"customer": {"first_name": customer.name, "last_name": "", "gender": "K", "birthday": "None", "customer_id": customer.id, "email": email}, "basket_voucher": "False", "finance": {"installment": "1", "bank": "Yapi Kredi", "received_at": "2014-04-14 10:23:09.213688", "host_ref_num": "", "card": "World Card", "charged": 0.00}, "tracker_code": tracker_code, "shipping_amount": "0.00", "payment_received_at": "2014-04-14 10:23:09.223071", "discount": {"source": "None", "percentage": "None", "code": "None"}, "orders": [{"billing_address": {"city": "\\u0130stanbul", "first_name": customer.name, "last_name": "", "district": "", "title": "", "phone": "", "postal_code": "34250", "address": "Markafoni Plaza", "township": "Gaziosmanpa\xc5\x9fa"}, "tracker_code": tracker_code, "is_subscribe": "False", "shipping_at": "2014-04-14 10:23:09.223071", "voucher": "0.00", "shipping_address": {"city": "\\u0130stanbul", "first_name": customer.name, "last_name": "", "district": "", "title": "", "phone": "", "postal_code": "34250", "address": "Markafoni Plaza", "township": "Gaziosmanpa\xc5\x9fa"}, "basketitems": [{"product": product.id, "sale_discount": "0", "gift_discount": "0", "price": price, "is_gift": False, "productmeta": pm.id, "nick": "", "voucher": "0.00", "persist_price": price, "external_id":1 , "base_selling_price": price, "tax_percent": pr.vat, "quantity": 1}]}]}, "type": "import_order_data"}

    import_order_data(dict_obj=data["data"])
    o = Order.objects.get(tracker_code=tracker_code)
    fa = o.financeaction_set.get()
    fa.is_sent = True
    fa.save()
    pi =o.picking_set.create()
    o.created_at = "2014-04-14 10:23:09.213688"
    o.save()


