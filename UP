import xlrd
from supplier.models import *
from brand.models import *

wb = xlrd.open_workbook("/home/enver.turkan/enmodatedarikcimarj.xls")
sh1 = wb.sheet_by_index(0)
for rownum in range(1,sh1.nrows):
    row = sh1.row_values(rownum)
    tedarikci = u"%s"%row[0]
    markalar = u"%s"%row[1]
    brands = markalar.split(", ")
    oran = row[2]
    ind_oran = row[3]

    supplier = Supplier.objects.get(name=tedarikci)
    for x in brands:
        b = Brand.objects.get(name=x)
        sb = supplier.supplierbrands_set.filter(brand=b)
        if sb:
            sb = sb[0]
            sb.consignment_commission_rate = float(oran)
            sb.save()
            print supplier.name, b.name, "oran girildi"
            if ind_oran:
                sb.discount_consignment_commission_rate = float(ind_oran)
                sb.save()
                print supplier.name, b.name, "ind oran girildi"
        else:
            sbn = supplier.supplierbrands_set.create(brand=b)
            sbn.consignment_commission_rate = float(oran)
            sbn.save()
            print supplier.name, b.name, "oran girildi"
            if ind_oran:
                sbn.discount_consignment_commission_rate = float(ind_oran)
                sbn.save()
                print supplier.name, b.name, "ind oran girildi"

